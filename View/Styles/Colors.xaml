<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:View.Styles">

    <Color x:Key="darkBlue">#395383</Color>
    <SolidColorBrush  x:Key="DarkBlue" Color="{StaticResource darkBlue}" />

    <Color x:Key="white">#DCE1E4</Color>
    <SolidColorBrush  x:Key="White" Color="{StaticResource white}" />

    <Color x:Key="gray">#1F2628</Color>
    <SolidColorBrush  x:Key="Gray" Color="{StaticResource gray}" />

    <Color x:Key="darkGray">#151A1A</Color>
    <SolidColorBrush  x:Key="DarkGray" Color="{StaticResource darkGray}" />

</ResourceDictionary>